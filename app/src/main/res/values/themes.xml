<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="ThemeOne" parent="Theme.MaterialComponents.DayNight.DarkActionBar">

        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/themeOnePrimary</item>
        <item name="colorPrimaryVariant">@color/themeOneLightColor</item>
        <item name="colorOnPrimary">@color/themeOnePrimaryTextColor</item>
        <item name="colorPrimaryDark">@color/themeOneDarkColor</item>
        <item name="chatlist_card_background_color">@color/themeOneLightColor</item>
        <item name="contact_card_background_color">@color/themeOneLightColor</item>
        <item name="title_text_color">@color/themeOnePrimary</item>

        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/themeOneSecondaryColor</item>
        <item name="colorSecondaryVariant">@color/themeOneSecondaryLightColor</item>
        <item name="colorOnSecondary">@color/themeOneSecondaryTextColor</item>

        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/themeOneLightColor</item>
    </style>

